type Player {
	id: ID!
	name: String!
	surname: String!
	position: String!
}
type Query {
	findAllPlayers: [Player]!
}

input PlayerDto {
    name: String!,
    surname: String!,
    position: Position!
}

type Mutation {
	createPlayer(player: PlayerDto): Player!
	deletePlayer(id: ID!): Boolean
}

enum Position {
  PG
  SG
  SF
  PF
  C
}